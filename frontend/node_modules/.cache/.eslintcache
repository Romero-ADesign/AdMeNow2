" element={<NavBar />}>\n            <Route index element={<Home ads={ads} tags={tags} />} />\n            <Route path=\"login\" element={<Login />} />\n            <Route path=\"mypage\" element={<MyPage ads={ads} tags={tags}/>} />\n            <Route path=\"ad/:id\" element={<DetailedAd  ads={ads}/>} />\n          </Route>\n        </Routes>\n      </BrowserRouter>\n    </div>\n  );\n}\n\nexport default App;\n","/home/grim/Development/code/debugging/AdMeNow2/frontend/src/reportWebVitals.js",[],[],"/home/grim/Development/code/debugging/AdMeNow2/frontend/src/MyPage.js",["51"],[],"import { useState, useEffect } from \"react\";\nimport React from \"react\";\nimport { Link } from \"react-router-dom\";\n\nfunction MyPage({ads, tags}) {\n  const [name, setName] = useState(\"\");\n  const [description, setDescription] = useState(\"\");\n  const [image, setImage] = useState(\"\");\n  const [price, setPrice] = useState(\"\");\n  const [selectedTagId, setSelectedTagId] = useState(null);\n  \n\n  function submitForm(e) {\n    e.preventDefault();\n    const configObj = {\n      method: \"POST\",\n      headers: { \"Content-Type\": \"application/json\" },\n      body: JSON.stringify({\n        name: name,\n        description: description,\n        image: image,\n        price: parseInt(price),\n        tag_id: selectedTagId\n      }),\n    };\n  \n    fetch(\"http://localhost:3000/api/ads\", configObj)\n      .then((res) => res.json())\n      .then((data) => console.log(data));\n  }\n\n  //placeholder until we can make user id 3 dynamic with auth\n  const myAds = ads.filter((ad) => ad.user_id === 3);\n\n  return (\n    <div className=\"MyPage\">\n      <h1>Post A New Ad</h1>\n      <form onSubmit={submitForm}>\n        <label htmlFor=\"name\">\n          Name your ad\n          <input\n            type=\"text\"\n            id=\"name\"\n            placeholder=\"example: 2006 Honda\"\n            onChange={(e) => setName(e.target.value)}\n          ></input>\n        </label>\n        <br />\n        <label htmlFor=\"description\">\n          Description of your product/service\n          <input\n            type=\"text\"\n            id=\"description\"\n            placeholder=\"list specifics of here...\"\n            onChange={(e) => setDescription(e.target.value)}\n          ></input>\n        </label>\n        <br />\n        <label htmlFor=\"image\">\n          Image of your product/service\n          <input\n            type=\"text\"\n            id=\"image\"\n            placeholder=\"url link to image...\"\n            onChange={(e) => setImage(e.target.value)}\n          ></input>\n        </label>\n        <br />\n        <label htmlFor=\"price\">\n          Price you are offerring\n          <input\n            type=\"text\"\n            id=\"price\"\n            placeholder=\"numbers only no decimals, symbols, etc...\"\n            onChange={(e) => setPrice(e.target.value)}\n          ></input>\n        </label>\n        <br />\n        <label htmlFor=\"tag\">\n          Select a tag\n          <select id=\"tag\" onChange={(e) => setSelectedTagId(e.target.value)}>\n            <option value=\"\">Select a tag</option>\n            {tags.map((tag) => (\n              <option key={tag.id} value={tag.id}>\n                {tag.name}\n              </option>\n            ))}\n          </select>\n        </label>\n        <br />\n        <input type=\"submit\"></input>\n      </form>\n      <h1>My Ads</h1>\n      {myAds.map((ad) => (\n        <div key={ad.id}>\n          <h2>{ad.name}</h2>\n          <p>{ad.description}</p>\n          <Link to={`/ad/${ad.id}`}>\n            <img src={ad.image} alt=\"product/service\" />\n          </Link>\n          <p>{ad.price}</p>\n        </div>\n      ))}\n    </div>\n  );\n}\n\nexport default MyPage;\n","/home/grim/Development/code/debugging/AdMeNow2/frontend/src/Home.js",["52","53","54"],[],"/home/grim/Development/code/debugging/AdMeNow2/frontend/src/Login.js",[],[],"/home/grim/Development